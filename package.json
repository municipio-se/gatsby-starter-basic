{
  "name": "@municipio/gatsby-starter-basic",
  "version": "0.0.0",
  "private": true,
  "description": "Basic Gatsby starter for Municipio",
  "scripts": {
    "build": "yarn wp-generate-schema-fragments && gatsby build",
    "clean": "gatsby clean",
    "dev": "yarn wp-generate-schema-fragments && gatsby develop --open",
    "format": "run-s format:*",
    "format:eslint": "eslint --fix '**/*.{js,jsx}' || true",
    "format:prettier": "prettier --write '**/*.{js,jsx,json,md,mdx,yml,html,css}'",
    "lint": "eslint '**/*.{js,jsx}'",
    "serve": "gatsby serve",
    "start": "yarn dev",
    "wp-generate-schema-fragments": "node scripts/wp-generate-schema-fragments"
  },
  "dependencies": {
    "@wsui/base": "^0.2.11",
    "@wsui/theme-standard": "^0.2.11",
    "@municipio/gatsby-theme-basic": "^1.3.0-next.2",
    "@whitespace/gatsby-plugin-cookie-consent": "^1.4.0-next.7",
    "@whitespace/gatsby-plugin-matomo": "^1.4.0-next.7",
    "gatsby": "^3.14.2",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "http-proxy-middleware": "^2.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1"
  },
  "resolutions": {
    "postcss": "^8",
    "postcss-preset-env": "^7"
  }
}
